ing: 0;\n    border: 0;\n    background-color: transparent;\n\n    &::-webkit-slider-runnable-track {\n        height: 5px;\n        background-color: @colorGray;\n        border: none;\n        -webkit-border-radius: 3px;\n        border-radius: 3px;\n    }\n\n    &::-webkit-slider-thumb {\n        -webkit-appearance: none;\n        border: none;\n        height: 16px;\n        width: 16px;\n        border-radius: 50%;\n        background-color: @colorGrayDark;\n        margin-top: -6px;\n    }\n\n    &:focus {\n        outline: none;\n    }\n\n    &:focus::-webkit-slider-thumb {\n        background-color: @colorLink;\n    }\n\n    &::-moz-range-track {\n        // width: 300px;\n        height: 5px;\n        background-color: @colorGray;\n        border: none;\n        -moz-border-radius: 3px;\n        border-radius: 3px;\n    }\n\n    &::-moz-range-thumb {\n        -webkit-appearance: none;\n        border: none;\n        height: 16px;\n        width: 16px;\n        border-radius: 50%;\n        background-color: @colorGrayDark;\n        margin-top: -4px;\n    }\n\n    &:focus {\n        outline: none;\n    }\n\n    &:focus::-moz-range-thumb {\n        background-color: @colorLink;\n    }\n}\n\n\n.dapp-identicon {\n    display: inline-block;\n    width: @gridWidth * 2;\n    height: @gridWidth * 2;\n    border-radius: 50%;\n    background-size: cover;\n    background-positon: 50% 50%;\n    box-shadow: inset rgba(255,255,255,0.2) 0 2px 2px, inset rgba(0,0,0,0.3) 0 -1px 8px;\n    box-shadow: inset rgba(255, 255, 255, 0.4) 0 4px 8px, inset rgba(0, 0, 0, 0.6) 0 -4px 12px;\n    cursor: help;\n    \n    &.dapp-tiny {\n        width: floor(@gridWidth / 1.5);\n        height: floor(@gridWidth / 1.5);\n        box-shadow: inset rgba(255, 255, 255, 0.4) 0 1px 1px, inset rgba(0, 0, 0, 0.3) 0 -1px 2px;\n    }\n    &.dapp-small {\n        width: @gridWidth;\n        height: @gridWidth;\n        box-shadow: inset rgba(255, 255, 255, 0.4) 0 2px 2px, inset rgba(0, 0, 0, 0.4) 0 -2px 4px;\n    }\n    &.dapp-medium {\n        width: floor(@gridWidth * 1.5);\n        height: floor(@gridWidth * 1.5);\n        box-shadow: inset rgba(255, 255, 255, 0.4) 0 4px 4px, inset rgba(0, 0, 0, 0.5) 0 -4px 6px;\n    }\n    &.dapp-icon-loading {\n        background-image: url('identicon-load.gif');\n    }\n}\n\n\n.dapp-modal-buttons {\n    position: relative;\n    .display(flex);\n    .justify-content(flex-end);\n    margin-top: @defaultMargin;\n    // background: @colorWhite;\n\n    button, a, a:visited {\n        .flex(1);\n        margin: 0 auto;\n        max-width: 150px;\n        height: 2*@gridHeight;\n        line-height: 2*@gridHeight;\n        font-family: @fontFamily;\n        font-size: 0.9em;\n        text-transform: uppercase;\n        font-weight: 400;\n        \n        &.dapp-primary-button {\n            font-weight: 600;\n        }\n    }\n\n    &.no-margins {\n        margin: 0;\n    }\n}\n\n.dapp-selector,\n.dapp-select-gas-price {\n    display: inline-block;\n    position: relative;\n\n    span:first-of-type {\n        font-size: 1.3em;\n        font-weight: 400;\n        color: @colorTextSecondary;\n\n        span {\n            font-size: 0.6em;\n        }\n    }\n\n    input {\n        width: 100%;\n        margin-bottom: @defaultMargin*2;\n\n        & + span,\n        & + span + span {\n            position: absolute;\n            top: 60px;\n            text-transform: uppercase;\n            font-size: 1em;\n            font-weight: 400;\n        }\n        & + span {\n            position: absolute;\n            left: 0;\n        }\n        & + span + span {\n            text-align: right;\n            right: 0;\n        }\n    }\n\n}\n\n@keyframes shrinkText {\n    from {\n        font-size: 150%;\n        opacity: 0.5;\n    }\n    to {\n        font-size: 100%;\n        opacity: 1;\n    }\n}\n\n@keyframes opacityText {\n    from {\n        color: @colorGrayLight;                \n    }\n    to {\n        color: @colorGrayDark;        \n    }\n}\n\n.dapp-address-input,\n.dapp-select-account {\n    position: relative;\n\n    input,\n    select {\n        .dapp-input;\n        width: 440px;\n        z-index: 1;\n        margin-top: 0;\n        height: @gridHeight*2;\n        padding-left: @gridWidth*1.3;\n        padding-right: 0;\n        transition-delay: 0s;\n        transition: background-color ease-in-out 1s, color ease-in-out 1s;\n\n        &.dapp-large {\n            font-size: 1.5em;\n            padding-left: @gridWidth*1.5;\n            height: auto;\n            line-height: floor(@gridHeight * 1.6);\n\n            & + .dapp-identicon {\n                top: 6px;\n                width: @gridWidth;\n                height: @gridWidth;\n            }\n            & + .icon-shield {\n                top: 11px;\n            }\n        }\n\n        &.dapp-error {\n            border-color: @colorError;\n        }\n\n    }\n    select {\n        width: auto;\n        -webkit-appearance: none;\n        -ms-appearance: none;\n        -o-appearance: none;\n        appearance: none;\n        border-radius: 0;\n        // padding-top: @gridWidth * 0.2;\n        // padding-bottom: @gridWidth * 0.2;\n        height: floor(@gridHeight * 2);\n        line-height: floor(@gridHeight * 1);\n\n        & + .dapp-identicon {\n            top: 4px;\n        }\n    }\n\n    .dapp-identicon {\n        z-index: 2;\n        position: absolute;\n        top: 4px;\n        left: 8px;\n        width: @gridWidth/1.2;\n        height: @gridWidth/1.2;\n    }\n\n    .no-identicon {\n        z-index: 2;\n        position: absolute;\n        top: 10px;\n        left: 15px;\n        color: @colorLink;\n    }\n\n    .icon-shield {\n        position: absolute;\n        top: 6px;\n        left: 13px;\n        font-size: 1.4em;        \n        color: @colorError;\n    }\n\n    .dapp-non-checksum {\n        background-color: lighten(@colorYellow, 30%);\n        cursor: help;\n    }\n\n    .dapp-address-label {\n        position: absolute;\n        bottom: @gridHeight * -1;\n        left: 0;\n        right: 0;\n        text-align: center;\n        font-size: 70%;\n        font-weight: 600;\n        text-transform: uppercase;\n        opacity: 0.6;\n    }\n\n     \n\n    &.has-name {\n        .ens-name {\n            position: absolute;\n            left: 40px;\n            top: 0px;\n            color: @colorLink;\n            animation-name: shrinkText;\n            animation-duration: 0.1s;\n            animation-timing-function: ease-out;\n        }\n\n        input:not(:focus) {\n            font-size: 66%;\n            padding-top: 17px;\n            color: @colorGrayDark;\n            font-weight: 400;\n            padding-bottom: 5px;\n            animation-name: opacityText;\n            animation-duration: 1s;\n            animation-timing-function: ease-out;\n        }\n\n        input:focus + .ens-name {\n            display: none;\n            color: @colorGrayDark;\n            font-size: 66%;\n        }\n    }\n\n    \n}\n\n.dapp-output {\n    display: flex;\n    flex: 1;\n    max-width: 100%;\n\n    em {\n        opacity: 0.4;\n    }\n\n    span.address {\n        font-size: 0.9 em;\n        font-weight: 600;\n        position: relative;\n        padding-left: 24px;\n        .dapp-identicon.dapp-tiny {\n            position: absolute;\n            top: -2px;\n            left: 0;\n        }\n}\n\n\n}\n\n\n\n","@import 'lib/lesshat.import.less';\n@import 'containers.import.less';\n@import 'elements.import.less';\n\n@gridWidth: 32px;\n@gridHeight: 18.4px;\n\n@defaultMargin: @gridWidth/2;\n@defaultPaddingVertical: @gridHeight;\n@defaultPaddingHorizontal: @gridWidth;\n\n@colorWhite: #FAFAFA;\n@colorBlack: #111;\n@colorGray: #dddcdb;\n@colorGrayLight: lighten(#ecebe6, 4%);\n@colorGrayDark: darken(@colorGray, 40%);\n@colorError: #C20E25;\n\n@colorBackgroundOverlay: @colorBlack;\n\n\n@colorTextPrimary: @colorBlack;\n@colorTextSecondary: #827a7a;\n\n@colorLink: #4A90E2;\n\n@fontFamily: 'Source Sans Pro', 'Helvetica Neue', Helvetica, Arial, Sans;\n\n// animations\n@animationSpeed: 400ms;\n\n"]}}